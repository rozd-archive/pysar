<?xml version="1.0" ?>
<mx:WindowedApplication initialize="createButton()"  height="100%"
xmlns:mx="http://www.adobe.com/2006/mxml" layout="absolute" xmlns:aswing="org.aswing.*">
	<mx:Script>
		<![CDATA[
			import org.aswing.JCheckBox;
			import org.aswing.JSlider;
			import mx.core.UIComponent;
			import org.aswing.JButton;
			
			private var myButton:*;
			
			public var child:UIComponent;
			
			public function createButton():void
			{
				myButton = new JSlider();
				myButton.setSizeWH(200,30);
				myButton.setLocationXY(10,10);
				child = new UIComponent();
				child.addChild(myButton);
				addChild(child);
				
				var slider:JSlider = new JSlider();
			}
		]]>
	</mx:Script>
	
</mx:WindowedApplication>